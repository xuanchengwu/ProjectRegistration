
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>compareError</title>

    <style>

        h2 {
            color: white;
            text-align: center;
        }

        html {
            height: 100%
        }

        button {
            background-color: white;
            color: lightblue;
            border: 2px solid lightblue;
            border-radius: 5px;
            padding: 10px 20px;
            text-align: center;
            display: block;
            margin: 20px auto;
        }

        body {
            min-height: 100%;
            background: #f093fb;
            background: -webkit-linear-gradient(to bottom right, rgba(0,0,128,1),rgba(135,206,250,1));
            /* W3C, IE 10+/ Edge, Firefox 16+, Chrome 26+, Opera 12+, Safari 7+ */
            background: linear-gradient(to bottom right, rgba(0,0,128,1),rgba(135,206,250,1));
        }
    </style>

</head>
<body>

    <h2>An error occurred: @ViewBag.ErrorMessage</h2>
    

    <button onclick="window.history.back();">Go back</button>




</body>
</html>
